{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "OpaDotNet.Wasm/*.csproj"
          ],
          "src": "../src"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*.{md,yml}",
          "api/**.{md,yml}",
          "articles/**.{md,yml}"
        ],
        "exclude": [ "_site/**", "**/obj/**", "**.meta" ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**"
        ],
        "exclude": [ "_site/**", "**/obj/**", "**.meta" ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "OpaDotNet",
      "_appName": "OpaDotNet"
    },
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
